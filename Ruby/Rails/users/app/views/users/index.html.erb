<table>
    <tr>
      <th>
          id
      </th>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Age
        </th>
        <th>
            Created At
        </th>
        <th>
            Updated At
        </th>
    </tr>
    <% for user in @users%>
    <tr>
      <td><%= user.id%></td>
      <td><%= user.first%></td>
      <td><%= user.last%></td>
      <td><%= user.age%></td>
      <td><%= user.created_at%></td>
      <td><%= user.updated_at%></td>
    </tr>
    <%end%>
</table>
